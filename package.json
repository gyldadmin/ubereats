{
  "name": "gyld-native",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "build:e2e:ios": "detox build -c ios.sim.release",
    "test:e2e:ios": "E2E=true detox test -c ios.sim.release --cleanup",
    "build:e2e:android": "detox build -c android.emu.release",
    "test:e2e:android": "E2E=true detox test -c android.emu.release --cleanup"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@supabase/supabase-js": "^2.45.4",
    "expo": "~51.0.36",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.29",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.10",
    "expo-image-picker": "~15.1.0",
    "expo-notifications": "~0.28.19",
    "expo-random": "~14.0.1",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native/babel-preset": "^0.80.2",
    "@testing-library/jest-native": "^5.4.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~18.2.6",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^30.0.5",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "metro-react-native-babel-preset": "^0.77.0",
    "typescript": "~5.3.3",
    "detox": "^20.19.3",
    "jest-circus": "^30.0.5",
    "@testing-library/react-native": "^12.1.5"
  },
  "private": true
}
